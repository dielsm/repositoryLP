    /**
     * opção de menu - sair, caso queira sair, será chamado o metodo para fechar
     * os objetos e logo após a aplicação será encerrada
     *
     * @param evt
     */
    private void jmiExitActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jmiExitActionPerformed
        int reply = JOptionPane.showConfirmDialog(null, "Voce realmente deseja sair?", "Sair", JOptionPane.YES_NO_OPTION);
        if (reply == JOptionPane.YES_OPTION) {
            table.disconnectFromDatabase();
            System.exit(0);
        }
    }//GEN-LAST:event_jmiExitActionPerformed

